// Tính S(n) = 1^2 + 2^2 + 3^2 + ... + n^2

#include <stdio.h> // thư viện

int main () {
    int n; // biến n kiểu int
    long long a = 0; // biến a kiểu long long 
    scanf ("%d", &n); // nhập vào n
    for (int i = 1; i <= n; i++) { // chạy từ 1 > n
        a += i * i; // cộng dồn i^2 vào a
    }
    printf ("%lld ", a); // in ra a
    return 0; // kết thúc
}
